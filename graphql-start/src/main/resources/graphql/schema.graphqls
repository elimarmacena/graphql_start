type Query{
  tattooById(id: ID): Tattoo
  tattooByAuthor(author: String): [Tattoo]
  studioById(id: ID): Studio
}

type Tattoo{
  id: ID
  url: String
  color: Boolean
  author: Author
  city: String
  state: String
}

type Studio{
  id: ID
  artists: [Author]
}

type Author{
  id: ID
  name: String
  instagramUser: String
}